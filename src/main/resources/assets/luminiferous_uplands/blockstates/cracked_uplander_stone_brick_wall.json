{
    "multipart": [
        {   "when": { "up": "true" },
            "apply": { "model": "$modid:block/walls/cracked_uplander_stone_brick_wall_post" }
        },
        {   "when": { "north": "low" },
            "apply": { "model": "$modid:block/walls/cracked_uplander_stone_brick_wall_side", "uvlock": true }
        },
        {   "when": { "east": "low" },
            "apply": { "model": "$modid:block/walls/cracked_uplander_stone_brick_wall_side", "y": 90, "uvlock": true }
        },
        {   "when": { "south": "low" },
            "apply": { "model": "$modid:block/walls/cracked_uplander_stone_brick_wall_side", "y": 180, "uvlock": true }
        },
        {   "when": { "west": "low" },
            "apply": { "model": "$modid:block/walls/cracked_uplander_stone_brick_wall_side", "y": 270, "uvlock": true }
        },
        {   "when": { "north": "tall" },
            "apply": { "model": "$modid:block/walls/cracked_uplander_tall_stone_brick_wall_side", "uvlock": true }
        },
        {   "when": { "east": "tall" },
            "apply": { "model": "$modid:block/walls/cracked_uplander_tall_stone_brick_wall_side", "y": 90, "uvlock": true }
        },
        {   "when": { "south": "tall" },
            "apply": { "model": "$modid:block/walls/cracked_uplander_tall_stone_brick_wall_side", "y": 180, "uvlock": true }
        },
        {   "when": { "west": "tall" },
            "apply": { "model": "$modid:block/walls/cracked_uplander_tall_stone_brick_wall_side", "y": 270, "uvlock": true }
        }
    ]
}
